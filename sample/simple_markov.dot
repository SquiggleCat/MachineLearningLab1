/* Markov model 
Build with dot -Tsvg simple_markov.dot > simple_markov.svg */

digraph finite_state_machine {
	fontname="Helvetica,Arial,sans-serif"
	node [fontname="Helvetica,Arial,sans-serif"]
	edge [fontname="Helvetica,Arial,sans-serif"]
	rankdir=LR;
	node [shape = circle];
	A -> C [label = "0.5"];
	A -> B [label = "0.5"];
	B -> G [label = "1"];
	C -> E [label = "0.2"];
	C -> D [label = "0.8"];
	D -> F [label = "0.9"];
	D -> D [label = "0.1"];
	E -> E [label = "0.6"];
	E -> D [label = "0.4"];
	F -> G [label = "1"];
	G -> A [label = "0.1"];
	G -> D [label = "0.9"];
}
